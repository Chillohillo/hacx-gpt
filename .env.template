# Kickbase Ultimate Analyzer - Environment Configuration
# Kopiere diese Datei zu .env und fülle deine echten Werte ein

# Kickbase Login Credentials
KICKBASE_EMAIL=deine@email.com
KICKBASE_PASSWORD=dein_passwort
KICKBASE_LEAGUE_ID=deine_liga_id

# AI API Keys
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GROK_API_KEY=grok-...

# Cloud Storage Credentials
GOOGLE_CREDENTIALS_PATH=path/to/google-service-account.json
AWS_ACCESS_KEY_ID=AKIA...
AWS_SECRET_ACCESS_KEY=...

# Optional: Database Configuration
DATABASE_URL=sqlite:///kickbase_data.db

# Optional: Notification Services
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/...
TELEGRAM_BOT_TOKEN=...
TELEGRAM_CHAT_ID=...

# Optional: Web Scraping
USER_AGENT=Mozilla/5.0 (compatible; KickbaseAnalyzer/1.0)

# Security Settings (für Kali Linux)
ENCRYPTION_KEY=your-32-char-encryption-key-here
USE_ENCRYPTION=false

# Logging Level
LOG_LEVEL=INFO